,"    const_cnt[word] = const_cnt.get(word, 0) + 1\n","\n","def word_graph(cnt, max_words=10):\n","    # 폰트 지정\n","    plt.rc('font', family=\"NanumBarunGothic\")\n","    # 토큰화된 토큰의 빈도수 순으로 오름차순 정렬.\n","    sorted_w = sorted(cnt.items(), key=lambda kv: kv[1])\n","    # 빈도수를 그래프로 나타낼 갯수를 max_words로 지정 \n","    print(sorted_w[-max_words:])\n","    # 현재 max_words가 10 이므로 상위 10개의 토큰이 표출\n","    # n에는 토큰 이름이, w에는 해당 토큰의 빈도수가 담긴 튜플\n","    n, w = zip(*sorted_w[-max_words:])\n","    print(n, w)\n","    # 토큰들로 그래프 작성\n","    plt.barh(n,w)\n","    plt.show()\n","\n","word_graph(const_cnt, max_words=20)"],"metadata":{"id":"aIBEhBJaC_Sy","executionInfo":{"status":"aborted","timestamp":1668140599909,"user_tz":-540,"elapsed":256,"user":{"displayName":"YEJIN NA","userId":"03025425295537444005"}}},"id":"aIBEhBJaC_Sy","execution_count":null,"outputs":[]},{"cell_type":"code","source":["from wordcloud import WordCloud\n","wordcloud = WordCloud(\n","    font_path = fontpath,\n","    max_font_size = 100,\n","    width = 800, #이미지 너비 지정\n","    height = 400, #이미지 높이 지정\n","    background_color='black', #이미지 배경색 지정\n","    max_words=50)\n","\n","wordcloud.generate_from_frequencies(const_cnt) #원문이 아닌 형태소 분석 결과로부터 워드클라우드를 생성\n","# wordcloud.to_file(\"const.png\") #생성한 이미지를 파일로 저장\n","\n","plt.axis(\"off\")\n","plt.imshow(wordcloud, interpolation='bilinear')\n","plt.show()"],"metadata":{"id":"GIMjjxVmC_QA","executionInfo":{"status":"aborted","timestamp":1668140599912,"user_tz":-540,"elapsed":259,"user":{"displayName":"YEJIN NA","userId":"03025425295537444005"}}},"id":"GIMjjxVmC_QA","execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["# 디씨 코인갤러리 크롤링"],"metadata":{"id":"tS3Xy6cDDS0k"},"id":"tS3Xy6cDDS0k"},{"cell_type":"code","source":["from urllib.request import urlopen\n","from bs4 import BeautifulSoup\n","from urllib.parse import quote\n","import requests\n","import pandas as pd"],"metadata":{"id":"jaNhgNSSC_Nw","executionInfo":{"status":"aborted","timestamp":166814059991