lSoup\n","import time\n","# 리뷰의 경우 페이지를 넘겨가며 크롤링 해야하기 때문에 selenium 사용\n","from selenium import webdriver\n","from selenium.webdriver.common.by import By\n","from selenium.webdriver.common.keys import Keys\n","\n","# 담을 리스트 초기화\n","review_title_list = [] \n","review_star_list = []\n","review_time_list = []\n","review_content_list = []\n","review_link_list = []\n","# selenium을 통해 드라이버설정 \n","wemakeprice_review_driver = webdriver.Chrome('chromedriver', options=options)\n","wemakeprice_review_driver.implicitly_wait(5)\n","# 100개의 상품 모두 진행하고 싶지만 오래걸리기 때문에 상위 5개 만으로 실습 (5개만해도 대략 4~5분의 시간이 소요)\n","for link in wemakeprice_link_list[:5]:\n","  print(link)\n","  url = link\n","  # selenium으로 페이지 open\n","  wemakeprice_review_driver.get(url)\n","  # open된 페이지의 소스를 가져옴\n","  source = wemakeprice_review_driver.page_source\n","  # 리뷰의 페이지 초기화\n","  count = 0\n","  # 99999 페이지까지 크롤링 (다하겠다.)\n","  while count < 99999:\n","    # 앞서 open한 페이지의 소스를 bs4로 파싱\n","    soup = BeautifulSoup(source, \"lxml\")\n","    # 리뷰 리스트 가 담겨있는 태그 찾아서 변수화\n","    review_list = soup.find(\"div\", attrs = {\"class\": \"review_list\"})\n","    # 리뷰 리스트가 담겨있는 태그가 없으면 리뷰가 없는것,\n","    if review_list == None:\n","      print(\"리뷰 없음\")\n","      # 반복문 종료\n","      break\n","    # 리뷰 리스트 태그 중 ul 태그 찾음 \n","    review_ul = review_list.find(\"ul\")\n","    # 해당 ul 태그중 li태그 모두 찾아 리스트화\n","    review_li = review_ul.find_all(\"li\")\n","    # li 태그 리스트를 통해 반복문 \n","    for li in review_li:\n","      # 필요한 값들 찾아서 변수화\n","      title = li.find(\"button\", attrs = {\"class\": \"name \"})\n","      write_time = li.find(\"span\", attrs = {\"class\": \"time\"})\n","      star = li.find(\"strong\", attrs = {\"class\": \"grade\"})\n","      content = li.find(\"p\", attrs = {\"class\": \"post\"})\n","      # li 태그가 리뷰 말고 이하에 이미지를 표현하는 곳에 하나 더있어서 찾고자 하는 요소들이 없을 경우에는 크롤링 대상이 아님\n","      if title == None:\n","        # title이 찾고자 하는 요소가 없을경우 이하 실행 X\n","        continue\n","\n","\n","      # title이 찾고자 하는 요소가 있을경우 가져오기\n","      titleText = title.text\n","      writeTimeText = write_time.text\n","      starText = star.text\n","      # 리뷰 내용이 없이 리뷰등록된 사례가 있어 리뷰 내용이 없을시 null 추가\n","      if content == None:\n","        print(\"리뷰 내용 없음\")\n","        contentText = \"null\"\n","      else :\n","        # 리뷰 내용이 있으면 text 추가\n","        contentText = content.text\n","        \n","      print(titleText)\n","      print(writeTimeText)\n","      print(starText)\n","      print(contentText)\n","      \n","      # 리스트에 찾은 요소들 추가\n","      review_title_list.append(titleText)\n","      review_star_list.append(starText)\n","      review_time_list.append(writeTimeText)\n","      review_content_list.append(contentText)\n","      # 나중에 dataframe의 조인을 위해서 리뷰의 url 리스트 추가  \n","      review_link_list.append(url)\n","\n","      print(\"-----------\"*30)\n","      \n","    try:  \n","      # 다음으로 넘어가는 버튼 XPath로 탐색\n","      next_btn = wemakeprice_review_driver.find_element(By.XPATH , '//*[@class=\"paging_comm\"]//a[@class=\"ico btn_next\"]')\n","      print(next_btn.text)\n","      print(next_btn.get_attribute(\"data-page\"))\n","      # 다음으로 넘어가는 버튼 탐색\n","      next_btn.send_keys(Keys.RETURN)\n","      # 페이지 소스 로딩을 위한 잠시대기 (없이 크롤링이 가능한 경우 대기는 없는것이 좋음)\n","      # 이번 케이스의 경우 잠시 대기가 없으면 데이터가 소수 유실됨\n","      time.sleep(1)\n","      # 페이지의 소스를 바꿔줌\n","      source = wemakeprice_review_driver.page_source\n","      # 리뷰페이지 카운트를 한번 올려줌\n","      count += 1\n","    except Exception as e: \n","      # find_element에서 해당되는 버튼의 xpath값을 찾지 못한경우 에러 발생\n","      # 에러가 발생했다는 것은 다음 페이지로 가는 버튼이 없다는 것 = 리뷰페이지의 마지막임\n","      print(\"리뷰 끝\")\n","      # 에러가 발생하는 것을 try ~ except로 잡아서 리뷰페이지 반복문을 종료\n","      break\n"],"metadata":{"id":"Ya0hmtZbDpd_","executionInfo":{"status":"aborted","timestamp":1668140599939,"user_tz":-540,"elapsed":282,"user":{"displayName":"YEJIN NA","userId":"03025425295537444005"}}},"id":"Ya0hmtZbDpd_","execution_count":null,"outputs":[]},{"cell_type":"code","source":["import pandas as pd\n","# 데이터 프레임제